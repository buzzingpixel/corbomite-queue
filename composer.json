{
    "name": "buzzingpixel/corbomite-queue",
    "version": "1.0.0",
    "description": "Corbomite queue",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "buzzingpixel/corbomite-db": "^1.0.2",
        "buzzingpixel/corbomite-di": "^1.0",
        "buzzingpixel/corbomite-migrations": "^1.0",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "doctrine/coding-standard": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^8.1",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\queue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "corbomite\\tests\\": "tests/"
        }
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php",
        "cliActionConfigFilePath": "src/actionConfig.php"
    }
}
